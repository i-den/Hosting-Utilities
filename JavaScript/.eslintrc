{
    "env": {
        "browser": true,
        "es6": true
        //"jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            4
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "warn",
            "always"
        ],
        // Custom
        "no-console": [
            "off"
        ],
        "no-debugger": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn"
    }
}
